{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^11.38.2",
        "laravel/sanctum": "^4.0.7",
        "laravel/telescope": "^5.3",
        "laravel/tinker": "^2.10",
        "resend/resend-php": "^0.15.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.40",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5",
        "pestphp/pest": "^3.7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "bun i && bun run build",
            "@php artisan migrate --force --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "ncu -u",
            "bun i && bun run build",
            "@php artisan migrate --graceful --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -k -c \"#aaff00,#93c5fd,#c4b5fd,#b2b2b2,#fb7185\" \"maildev\" \"php artisan serve\" \"php artisan queue:listen --queue=default\" \"php artisan inertia:start-ssr\" \"bun run dev\" --names=mailer,server,queue,inertia-ssr,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
